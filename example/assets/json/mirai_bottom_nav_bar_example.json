                        }
                    },
                    {
                        "type": "navigationBarItem",
                        "label": "Calendar",
                        "icon": {
                            "type": "icon",
                            "iconType": "material",
                            "icon": "calendar_month",
                            "size": 24
                        }
                    },
                    {
                        "type": "navigationBarItem",
                        "label": "Account",
                        "icon": {
                            "type": "icon",
                            "iconType": "cupertino",
                            "icon": "profile_circled",
                            "size": 24
                        }
                    }
                ]
            }
        }
    }
}