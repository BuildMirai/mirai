{
    "type": "scaffold",
    "appBar": {
        "type": "appBar",
        "title": {
            "type": "text",
            "data": "Tabbar Widget",
            "align": "center",
            "style": {
                "fontSize": 21
            }
        },
        "leading": {
            "type": "iconButton",
            "child": {
                "type": "icon",
                "iconType": "material",
                "icon": "arrow_back",
                "color": "#ffffff"
            },
            "onPressed": {
                "actionType": "navigate",
                "navigationType": "screen",
                "navigationStyle": "pop"
            }
        },
        "backgroundColor": "#99ccff"
    },
    "body": {
        "type": "defaultTabController",
        "length": 3,
        "child": {
            "type": "tabBar",
            "tabs": [
                {
                    "type": "icon",
                    "iconType": "material",
                    "icon": "male",
                    "size": 40,
                    "color": "#000000"
                },
                {
                    "type": "icon",
                    "iconType": "material",
                    "icon": "female",
                    "size": 40,
                    "color": "#000000"
                },
                {
                    "type": "icon",
                    "iconType": "material",
                    "icon": "child_care",
                    "size": 40,
                    "color": "#000000"
                }
            ]
        }
    }
}