{
    "type": "scaffold",
    "appBar": {
        "type": "appBar",
        "title": {
            "type": "text",
            "data": "Mirai Floating Action Button",
            "align": "center",
            "style": {
                "color": "#ffffff",
                "fontSize": 21
            }
        },
        "backgroundColor": "#99ccff",
        "leading": {
            "type": "iconButton",
            "child": {
                "type": "icon",
                "iconType": "material",
                "icon": "arrow_back",
                "size": 50,
                "color": "#ffffff"
            },
            "onPressed": {
                "actionType": "navigate",
                "navigationType": "screen",
                "navigationStyle": "pop"
            }
        }
    },
    "body": {},
    "floatingActionButton": {
        "type": "floatingActionButton",
        "buttonType": "large",
        "child": {
            "type": "text",
            "data": "Next"
        }
    }
}