{
    "type": "scaffold",
    "appBar": {
        "type": "appBar",
        "title": {
            "type": "text",
            "data": "Tab Bar View",
            "align": "center",
            "style": {
                "fontSize": 21
            }
        },
        "leading": {
            "type": "iconButton",
            "child": {
                "type": "icon",
                "iconType": "material",
                "icon": "arrow_back",
                "color": "#ffffff"
            },
            "onPressed": {
                "actionType": "navigate",
                "navigationType": "screen",
                "navigationStyle": "pop"
            }
        },
        "backgroundColor": "#99ccff"
    },
    "body": {
        "type": "defaultTabController",
        "length": 3,
        "child": {
            "type": "tabBarView",
            "children": [
                {
                    "type": "container",
                    "color": "#82ac64",
                    "child": {
                        "type": "column",
                        "mainAxisAlignment": "start",
                        "crossAxisAlignment": "start",
                        "children": [
                            {
                                "type": "text",
                                "data": "Swipe left and right to see tab bar views.",
                                "align": "center",
                                "style": {
                                    "fontSize": 17,
                                    "color": "#ffffff"
                                }
                            }
                        ]
                    }
                },
                {
                    "type": "container",
                    "color": "#36374b",
                    "child": {
                        "type": "column",
                        "mainAxisAlignment": "start",
