                            {
                                "type": "text",
                                "data": "Swipe left and right to see tab bar views.",
                                "align": "center",
                                "style": {
                                    "fontSize": 17,
                                    "color": "#ffffff"
                                }
                            }
                        ]
                    }
                },
                {
                    "type": "container",
                    "color": "#ffff6a",
                    "child": {
                        "type": "column",
                        "mainAxisAlignment": "start",
                        "crossAxisAlignment": "start",
                        "children": [
                            {
                                "type": "text",
                                "data": "Swipe left and right to see tab bar views.",
                                "align": "center",
                                "style": {
                                    "fontSize": 17,
                                    "color": "#ffffff"
                                }
                            }
                        ]
