{
    "type": "scaffold",
    "appBar": {
        "type": "appBar",
        "title": {
            "type": "text",
            "data": "Scaffold Widget",
            "align": "center",
            "style": {
                "fontSize": 21
            }
        },
        "leading": {
            "type": "iconButton",
            "child": {
                "type": "icon",
                "iconType": "material",
                "icon": "arrow_back",
                "color": "#ffffff"
            },
            "onPressed": {
                "actionType": "navigate",
                "navigationType": "screen",
                "navigationStyle": "pop"
            }
        },
        "backgroundColor": "#99ccff"
    },
    "body": {},
    "floatingActionButton": {
        "type": "floatingActionButton",
        "backgroundColor": "#99ccff",
        "buttonType": "large",
        "child": {
            "type": "text",
            "data": "Next"
        }
    },
    "bottomNavigationBar": {
        "type": "bottomNavigationBar",
        "backgroundColor": "#99ccff",
        "items": [
            {
                "type": "navigationBarItem",
                "label": "Items",
                "icon": {
                    "type": "icon",
                    "iconType": "material",
                    "icon": "category",
                    "size": 40
                }
            },
            {
                "type": "navigationBarItem",
                "label": "People",
                "icon": {
                    "type": "icon",
                    "iconType": "material",
                    "icon": "people",
                    "size": 40
                }
            }
        ]
    }
}