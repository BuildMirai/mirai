{
    "type": "scaffold",
    "appBar": {
        "type": "appBar",
        "title": {
            "type": "text",
            "data": "Mirai App Bar",
            "align": "center",
            "style": {
                "color": "#ffffff",
                "fontSize": 21
            }
        },
        "backgroundColor": "#99ccff",
        "leading": {
            "type": "iconButton",
            "child": {
                "type": "icon",
                "iconType": "material",
                "icon": "arrow_back",
                "color": "#ffffff"
            },
            "onPressed": {
                "actionType": "navigate",
                "navigationType": "screen",
                "navigationStyle": "pop"
            }
        },
        "actions": [
            {
                "type": "textButton",
                "child": {
                    "type": "text",
                    "data": "Sign in"
                },
                "width": 100,
                "height": 50,
                "style": {
                    "foregroundColor": "#ffffff"
                }
            }
        ]
    },
    "body": {
        "type": "listView",
        "shrinkWrap": true,
        "children": [
            {
                "type": "sizedBox",
                "height": 25
            },
            {
                "type": "text",
                "data": "This is an Appbar example.",
                "style": {
                    "fontSize": 21,
                    "align": "center"
                }
            }
        ]
    }
}