{
    "type": "scaffold",
    "appBar": {
        "type": "appBar",
        "title": {
            "type": "text",
            "data": "Navigation"
        }
    },
    "body": {
        "type": "row",
        "mainAxisAlignment": "center",
        "crossAxisAlignment": "center",
        "children": [
            {
                "type": "column",
                "mainAxisAlignment": "center",
                "crossAxisAlignment": "center",
                "children": [
                    {
                        "type": "elevatedButton",
                        "child": {
                            "type": "text",
                            "data": "Push Navigation Example"
                        },
                        "style": {
                            "padding": {
                                "top": 8,
                                "left": 12,
                                "right": 12,
                                "bottom": 8
                            }
                        },
                        "onPressed": {
                            "actionType": "navigate",
                            "navigationStyle": "push",
                            "navigationType": "screen",
                            "widgetJson": {
                                "type": "exampleScreen",
                                "assetPath": "assets/json/navigator_example.json"
                            }
                        }
                    },
                    {
                        "type": "sizedBox",
                        "height": 12
                    },
                    {
                        "type": "elevatedButton",
                        "child": {
                            "type": "text",
                            "data": "pushNamed and Replace"
                        },
                        "style": {
                            "padding": {
                                "top": 8,
                                "left": 12,
                                "right": 12,
                                "bottom": 8
                            }
                        },
                        "onPressed": {
                            "actionType": "navigate",
                            "navigationStyle": "pushReplacementNamed",
                            "navigationType": "screen",
                            "routeName": "/detailsScreen"
                        }
                    },
                    {
                        "type": "sizedBox",
                        "height": 12
                    },
                    {
                        "type": "elevatedButton",
                        "child": {
                            "type": "text",
                            "data": "PushNamed - Details"
                        },
                        "style": {
                            "padding": {
                                "top": 8,
                                "left": 12,
                                "right": 12,
                                "bottom": 8
                            }
                        },
                        "onPressed": {
                            "actionType": "navigate",
                            "navigationStyle": "pushNamed",
                            "navigationType": "screen",
                            "routeName": "/detailsScreen"
                        }
                    },
                    {
                        "type": "sizedBox",
                        "height": 12
                    },
                    {
                        "type": "elevatedButton",
                        "child": {
                            "type": "text",
                            "data": "PushNamed and Remove Until"
                        },
                        "style": {
                            "padding": {
                                "top": 8,
                                "left": 12,
                                "right": 12,
                                "bottom": 8
                            }
                        },
                        "onPressed": {
                            "actionType": "navigate",
                            "navigationStyle": "pushNamedAndRemoveUntil",
                            "navigationType": "screen",
                            "routeName": "/homeScreen"
                        }
                    },
                    {
                        "type": "sizedBox",
                        "height": 12
                    },
                    {
                        "type": "elevatedButton",
                        "child": {
                            "type": "text",
                            "data": "Show Dialog"
                        },
                        "style": {
                            "padding": {
                                "top": 8,
                                "left": 12,
                                "right": 12,
                                "bottom": 8
                            }
                        },
                        "onPressed": {
                            "actionType": "navigate",
                            "navigationStyle": "push",
                            "navigationType": "dialog",
                            "widgetJson": {
                                "type": "alertDialog",
                                "content": {
                                    "type": "padding",
                                    "padding": {
                                        "top": 12,
                                        "left": 12,
                                        "right": 12,
                                        "bottom": 12
                                    },
                                    "child": {
                                        "type": "text",
                                        "data": "Discard draft?",
                                        "align": "center",
                                        "style": {
                                            "fontSize": 14
                                        }
                                    }
                                },
                                "actions": [
                                    {
                                        "type": "textButton",
                                        "child": {
                                            "type": "text",
                                            "data": "CANCEL"
                                        },
                                        "onPressed": {
                                            "actionType": "navigate",
                                            "navigationType": "screen",
                                            "navigationStyle": "pop"
                                        }
                                    },
                                    {
                                        "type": "sizedBox",
                                        "width": 8
                                    },
                                    {
                                        "type": "textButton",
                                        "child": {
                                            "type": "text",
                                            "data": "DISCARD"
                                        },
                                        "onPressed": {
                                            "actionType": "navigate",
                                            "navigationType": "screen",
                                            "navigationStyle": "pop"
                                        }
                                    },
                                    {
                                        "type": "sizedBox",
                                        "width": 12
                                    }
                                ]
                            }
                        }
                    }
                ]
            }
        ]
    }
}