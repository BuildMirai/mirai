{
    "type": "scaffold",
    "appBar": {
        "type": "appBar",
        "backgroundColor": "#4D00E9",
        "title": {
            "type": "text",
            "data": "Chat",
            "style": {
                "fontSize": 21,
                "color": "#ffffff"
            }
        }
    },
    "body": {
        "type": "listView",
        "physics": "never",
        "shrinkWrap": true,
        "children": [
            {
                "type": "sizedBox",
                "height": 25
            },
            {
                "type": "defaultTabController",
                "length": 3,
                "indicatorColor": "#4D00E9",
                "child": {
                    "type": "tabBar",
                    "tabs": [
                        {
                            "type": "icon",
                            "iconType": "cupertino",
                            "icon": "home",
                            "color": "#FC5632"
                        },
                        {
                            "type": "icon",
                            "iconType": "cupertino",
                            "icon": "chat_bubble",
                            "color": "#FC5632"
                        },
                        {
                            "type": "icon",
                            "iconType": "cupertino",
                            "icon": "profile_circled",
                            "color": "#FC5632"
                        }
                    ]
                }
            },
            {
                "type": "container",
                "color": "#FC5632",
                "clipBehavior": "hardEdge",
                "height": 700,
                "width": 400
            }
        ]
    }
}